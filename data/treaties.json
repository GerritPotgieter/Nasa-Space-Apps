    {
      "treatyId": "OST_1967",
      "treatyName": "Outer Space Treaty",
      "fullName": "Treaty on Principles Governing the Activities of States in the Exploration and Use of Outer Space, including the Moon and Other Celestial Bodies",
      "adoptedDate": "1967-01-27",
      "entryIntoForce": "1967-10-10",
      "depositary": "United States, United Kingdom, Soviet Union",
      "parties": 114,
      "unOosaUrl": "https://www.unoosa.org/oosa/en/ourwork/spacelaw/treaties/outerspacetreaty.html",
      "bindingOn": "ALL_PARTIES",
      "obligations": [
        {
          "obligationId": "OST_001",
          "articleNumber": "Article I",
          "obligationType": "FREEDOM_OF_EXPLORATION",
          "obligationText": "The exploration and use of outer space shall be carried out for the benefit and in the interests of all countries and shall be the province of all mankind. Outer space shall be free for exploration and use by all States without discrimination.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "ALL_SPACE_ACTIVITIES",
          "mandatory": true,
          "severity": 2,
          "severityRationale": "Foundational principle but violations rarely result in immediate sanctions",
          "potentialSanctions": [
            "Diplomatic protests",
            "International criticism",
            "UN General Assembly resolution"
          ],
          "verificationMethod": "Mission profile review",
          "requiredEvidence": ["Mission objectives document", "Public mission statement"],
          "complianceChecks": [
            "Mission serves peaceful purposes",
            "No exclusive territorial claims",
            "Results shared with international community"
          ]
        },
        {
          "obligationId": "OST_002",
          "articleNumber": "Article II",
          "obligationType": "NO_SOVEREIGNTY",
          "obligationText": "Outer space, including the moon and other celestial bodies, is not subject to national appropriation by claim of sovereignty, by means of use or occupation, or by any other means.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "ALL_SPACE_ACTIVITIES",
          "mandatory": true,
          "severity": 1,
          "severityRationale": "Core principle of international space law; violations could trigger UN Security Council action",
          "potentialSanctions": [
            "UN Security Council intervention",
            "International condemnation",
            "Potential economic sanctions",
            "Invalidation of claimed sovereignty"
          ],
          "verificationMethod": "Legal review of mission statements",
          "requiredEvidence": ["Mission documentation", "Public statements"],
          "complianceChecks": [
            "No sovereignty claims in mission documents",
            "No exclusive use declarations",
            "No territorial appropriation language"
          ]
        },
        {
          "obligationId": "OST_003",
          "articleNumber": "Article III",
          "obligationType": "INTERNATIONAL_LAW_COMPLIANCE",
          "obligationText": "States Parties shall carry on activities in the exploration and use of outer space in accordance with international law, including the Charter of the United Nations, in the interest of maintaining international peace and security.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "ALL_SPACE_ACTIVITIES",
          "mandatory": true,
          "severity": 1,
          "severityRationale": "Links space activities to broader international law framework",
          "potentialSanctions": [
            "State responsibility under international law",
            "UN Security Council action for peace/security violations",
            "International Court of Justice proceedings"
          ],
          "verificationMethod": "Comprehensive legal compliance review",
          "requiredEvidence": ["Mission authorization", "Legal compliance certification"],
          "complianceChecks": [
            "Activities comply with UN Charter",
            "No threat to international peace and security"
          ]
        },
        {
          "obligationId": "OST_004",
          "articleNumber": "Article IV",
          "obligationType": "WMD_PROHIBITION",
          "obligationText": "States Parties undertake not to place in orbit around the earth any objects carrying nuclear weapons or any other kinds of weapons of mass destruction, install such weapons on celestial bodies, or station such weapons in outer space in any other manner.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "ALL_SATELLITES",
          "mandatory": true,
          "severity": 1,
          "severityRationale": "Absolute prohibition; violation would constitute grave breach of international peace and security",
          "potentialSanctions": [
            "Immediate UN Security Council action",
            "International sanctions",
            "Forced removal of weapons",
            "Military response authorized under UN Charter"
          ],
          "verificationMethod": "Payload inspection and mission documentation review",
          "requiredEvidence": ["Payload manifest", "Export control documentation", "No weapons certification"],
          "complianceChecks": [
            "No nuclear weapons on board",
            "No biological weapons",
            "No chemical weapons",
            "No other WMD"
          ]
        },
        {
          "obligationId": "OST_005",
          "articleNumber": "Article IV (Celestial Bodies)",
          "obligationType": "PEACEFUL_USE_CELESTIAL_BODIES",
          "obligationText": "The establishment of military bases, installations and fortifications, the testing of any type of weapons and the conduct of military maneuvers on celestial bodies shall be forbidden.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "MISSIONS_TO_CELESTIAL_BODIES",
          "mandatory": true,
          "severity": 1,
          "severityRationale": "Demilitarization of celestial bodies is absolute",
          "potentialSanctions": [
            "UN Security Council enforcement action",
            "Forced dismantling of installations",
            "International sanctions"
          ],
          "verificationMethod": "Mission profile analysis",
          "requiredEvidence": ["Landing site plans", "Equipment manifest"],
          "complianceChecks": [
            "No military bases or installations",
            "No weapons testing",
            "No military maneuvers"
          ],
          "applicabilityCondition": "satellite.destination === 'MOON' || satellite.destination === 'CELESTIAL_BODY'"
        },
        {
          "obligationId": "OST_006",
          "articleNumber": "Article V",
          "obligationType": "ASTRONAUT_ASSISTANCE",
          "obligationText": "States Parties shall regard astronauts as envoys of mankind in outer space and shall render to them all possible assistance in the event of accident, distress, or emergency landing.",
          "bindingOn": "ALL_PARTIES",
          "triggers": "CREWED_MISSIONS",
          "mandatory": true,
          "severity": 2,
          "severityRationale": "Humanitarian obligation",
          "potentialSanctions": [
